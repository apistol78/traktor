				</item>
				<item>
					<id>{1A3B0A00-C535-47AD-85A8-D600113C7799}</id>
					<name>TargetSize</name>
				</item>
			</inputPins>
			<outputPins>
				<item>
					<id>{B2D07F69-0F28-4EBD-BC62-4F75F63B5AC1}</id>
					<name>Output</name>
					<type>Scalar</type>
				</item>
			</outputPins>
			<script><![CDATA[
			
float average = 0.0f;

for (int i = 0; i < 64; ++i)
{
	const float intensity = i / 63.0f;
	average += $Histogram[i].B * intensity;
}

average /= ($TargetSize.x * $TargetSize.y);

$Output = average;
			
			]]></script>
